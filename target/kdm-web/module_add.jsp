<%@page contentType="text/html" pageEncoding="UTF-8"%><%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><%@taglib prefix="form" uri="http://www.springframework.org/tags/form"%><%@include file="include/html_head.inc"%><link rel="stylesheet" href="<%=UI_PATH%>css/select2_metro.css" /><link rel="stylesheet" href="<%=UI_PATH%>css/DT_bootstrap.css" /><link rel="stylesheet" type="text/css"	href="<%=UI_PATH%>css/bootstrap-fileupload.css" /><!-- 继续在下面添加自己所需的JS/CSS资源 --></head><!-- END HEAD --><!-- BEGIN BODY --><body class="page-header-fixed page-full-width">	<%@include file="include/layout_top.inc"%>	<!-- BEGIN CONTAINER -->	<div class="page-container row-fluid">		<!-- BEGIN EMPTY PAGE SIDEBAR -->		<!-- END EMPTY PAGE SIDEBAR -->		<!-- BEGIN PAGE -->		<div class="page-content">			<!-- BEGIN SAMPLE PORTLET CONFIGURATION MODAL FORM-->			<!-- END SAMPLE PORTLET CONFIGURATION MODAL FORM-->			<!-- BEGIN PAGE CONTAINER-->			<div class="container-fluid">				<div class="row-fluid">					<!-- BEGIN PORTLET-->					<div id="tab_1_5" class="tab-pane">						<div class="portlet box green">							<div class="portlet-title">								<div class="caption">									<i class="icon-reorder"></i> 模块上传								</div>							</div>							<form id="fileupload"								action="<c:url value="/module/uploadmodele/"/>${appId}" method="POST"								enctype="multipart/form-data" class="form-horizontal">								<div class="span12">									<div class="portlet-body form">										<div class="control-group">											<label class="control-label">模块文件:</label>											<div class="controls">												<div class="fileupload fileupload-new"													data-provides="fileupload">													<div class="input-append">														<div class="uneditable-input">															<i class="icon-file fileupload-exists"></i> <span																class="fileupload-preview"></span>														</div>														<span class="btn btn-file"> <span															class="fileupload-new">选择上传文件</span> <span															class="fileupload-exists">Change</span> <input															type="file" name="file" id="fileIpt"/>														</span> <a href="#" class="btn fileupload-exists"															data-dismiss="fileupload">Remove</a>														<button type="submit"   id="uploadBtn"  class="btn green start">															<i class="icon-upload icon-white"></i> <span>上传</span>														</button>													</div>												</div>											</div>										</div>									</div>								</div>							</form>			<div class="portlet-body form">				<form:form class="form-horizontal" id="module_form"					modelAttribute="mfb" servletRelativeAction="/module/save">					   <form:hidden path="mappId" value="${mfb.mappId}" /> 						<form:hidden path="module.mid" value="${mfb.module.mid }" />						<form:hidden path="module.appId" value="${mfb.mappId}" />						 <div class="control-group">							<label class="control-label">模块名称:</label>							<div class="controls">								<form:input path="module.moduleName" class="span4 m-wrap"   value="${mfb.module.moduleName }"   />							</div>						</div>						<div class="control-group">							<label class="control-label">安装路径:</label>							<div class="controls">								<form:input path="module.installPath" class="span4 m-wrap"    value="${mfb.module.installPath }"   />							</div>						</div>						<div class="control-group">							<label class="control-label">启动脚本:</label>							<div class="controls">								<form:input path="module.startScript"    class="span4 m-wrap"   value="${mfb.module.startScript }"    />							</div>						</div>						<div class="control-group">							<label class="control-label">停止脚本:</label>							<div class="controls">								<form:input path="module.stopScript" class="span4 m-wrap"   value="${mfb.module.stopScript }"     />							</div>						</div>						<div class="control-group">							<label class="control-label">模块描述:</label>							<div class="controls">								<form:textarea path="module.descs" class="span4 m-wrap" rows="3"   value="${mfb.module.descs }"     />  							</div>						</div> 						<div class="form-actions"  >							<button id="save_btn" title="ok" type="button" class="btn green">								<i class="icon-ok"></i> 保存							</button>							<button title="cancel" type="button" class="btn green"  id="cancel_btn"								>取消</button>						</div>			</form:form>		</div>				</div>	</div>					<!-- END PORTLET-->				</div>			</div>		</div>		<!-- END PAGE CONTENT-->	</div>	<!-- END PAGE CONTAINER-->	<!-- END PAGE -->	<!-- END CONTAINER -->	<%@include file="include/layout_bottom.inc"%>	<script type="text/javascript"		src="<%=UI_PATH%>js/bootstrap-fileupload.js"></script>	<script type="text/javascript" src="<%=UI_PATH%>js/select2.min.js"></script>	<script>		jQuery(document).ready(function() {			App.init();	            //保存按钮			$('#save_btn').click(function(){				$('#module_form').submit();			});	            		      //上传按钮			$('#uploadBtn').click(function(e){				var fileSelected = $('.fileupload-preview').text();			    if('' == fileSelected) {			    	e.stopPropagation();			    	alert("先选择需要上传文件！");			    	return false;			    }			}); 	   				    $('#cancel_btn').click(function(){		    	window.location.href='<c:url value="/module/cancel/"/>'+${appId}		    });		      		});	</script></html>